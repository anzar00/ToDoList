<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Resources\Fonts\Oswald-Bold.ttf" />
    <None Remove="Resources\Fonts\Oswald-ExtraLight.ttf" />
    <None Remove="Resources\Fonts\Oswald-Light.ttf" />
    <None Remove="Resources\Fonts\Oswald-Medium.ttf" />
    <None Remove="Resources\Fonts\Oswald-Regular.ttf" />
    <None Remove="Resources\Fonts\Oswald-SemiBold.ttf" />
    <None Remove="Resources\Images\ToDoList.png" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Resources\Fonts\Oswald-Bold.ttf" />
    <Content Include="Resources\Fonts\Oswald-ExtraLight.ttf" />
    <Content Include="Resources\Fonts\Oswald-Light.ttf" />
    <Content Include="Resources\Fonts\Oswald-Medium.ttf" />
    <Content Include="Resources\Fonts\Oswald-Regular.ttf" />
    <Content Include="Resources\Fonts\Oswald-SemiBold.ttf" />
    <Content Include="Resources\Images\ToDoList.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

</Project>
